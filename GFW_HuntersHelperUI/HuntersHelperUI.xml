					<OnLoad>
						this:SetText(SEARCH);
						this:SetPoint("RIGHT", FHH_UIViewByDropDown, "LEFT", 17, 2)
						this:SetFrameLevel(this:GetFrameLevel()+1);
					</OnLoad>
					<OnEnterPressed>
						this:ClearFocus();
					</OnEnterPressed>
					<OnEscapePressed>
						this:ClearFocus();
					</OnEscapePressed>
					<OnTextChanged>
						FHH_UIFilter_OnTextChanged();
					</OnTextChanged>
					<OnEditFocusLost>
						this:HighlightText(0, 0);
						if ( this:GetText() == "" ) then
							this:SetText(SEARCH);
						end
					</OnEditFocusLost>
					<OnEditFocusGained>
						this:HighlightText();
						if ( this:GetText() == SEARCH ) then
							this:SetText("");
						end
					</OnEditFocusGained>
				</Scripts>
